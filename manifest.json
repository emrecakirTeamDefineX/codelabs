{
    "name": "Codelabs",
    "version": "1.0",
    "manifest_version": 1,
    "description": "Codelabs",
    "minimum_chrome_version": "18",
    "permissions": ["http://*/*", "https://*/*", "unlimitedStorage", "contextMenus", "cookies", "tabs", "notifications"],
    "content_security_policy": "default-src 'self' style-src 'self' 'unsafe-inline';"
  }